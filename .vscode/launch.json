{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üåê Debug Web (Default Browser)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "toolArgs": [
        "-d",
        "web-server",
        "--web-port=4000",
        "--web-hostname=localhost"
      ]
    },
