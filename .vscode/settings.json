{
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  "dart.lineLength": 100,
  "dart.closingLabels": true,
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.hotReloadOnSave": "always",
  "dart.debugExternalLibraries": false,
  "dart.debugSdkLibraries": false,
  "dart.evaluateGettersInDebugViews": true,
  "dart.flutterCreateAndroidLanguage": "kotlin",
  "dart.flutterCreateIOSLanguage": "swift",
  "dart.flutterCreatePlatforms": ["web", "android", "ios"],
  "dart.warnWhenEditingFilesInPubCache": true,
  "dart.warnWhenEditingFilesOutsideWorkspace": true,
  "files.watcherExclude": {
    "**/build/**": true,
    "**/.fvm/**": true,
    "**/node_modules/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/build": true,
    "**/*.g.dart": true,
    "**/*.freezed.dart": true,
    "**/*.gr.dart": true,
    "**/*.config.dart": true,
    "**/*.mocks.dart": true,
    ".fvm/flutter_sdk": true
  },
  "search.exclude": {
    "**/build/**": true,
    "**/.fvm/**": true,
    "**/node_modules/**": true,
    "**/*.g.dart": true,
    "**/*.freezed.dart": true,
    "**/*.gr.dart": true,
    "**/*.config.dart": true,
    "**/*.mocks.dart": true
  },
  "editor.formatOnSave": true,
  "editor.rulers": [80, 100],
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "json.schemas": [
    {
      "fileMatch": ["firebase.json"],
      "url": "https://raw.githubusercontent.com/firebase/firebase-tools/master/schema/firebase-config.json"
    }
  ],
  "emmet.includeLanguages": {
    "dart": "html"
  }
}